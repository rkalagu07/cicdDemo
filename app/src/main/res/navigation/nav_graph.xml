<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/roomRegisterFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.game.myapptest.ui.note.MainFragment"
        android:label="MainFragment">
        <action
            android:id="@+id/action_mainFragment_to_noteFragment"
            app:destination="@id/noteFragment" />
        <action
            android:id="@+id/action_mainFragment_to_registerFragment"
            app:destination="@id/registerFragment" />

    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.game.myapptest.ui.login.RegisterFragment"
        android:label="RegisterFragment">
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_registerFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:popUpTo="@id/registerFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.game.myapptest.ui.login.LoginFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register">
        <action
            android:id="@+id/action_loginFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:popUpTo="@id/registerFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.game.myapptest.ui.profile.ProfileFragment"
        android:label="ProfileFragment" />
    <fragment
        android:id="@+id/noteFragment"
        android:name="com.game.myapptest.ui.note.NoteFragment"
        android:label="NoteFragment" />
    <fragment
        android:id="@+id/roomLoginFragment"
        android:name="com.game.myapptest.ui.room.RoomLoginFragment"
        android:label="fragment_room_login"
        tools:layout="@layout/fragment_room_login" >
        <action
            android:id="@+id/action_roomLoginFragment_to_roomMainFragment"
            app:destination="@id/roomMainFragment" />
        <action
            android:id="@+id/action_roomLoginFragment_to_roomMainFragment2"
            app:destination="@id/roomMainFragment"
            app:popUpTo="@id/roomRegisterFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/roomMainFragment"
        android:name="com.game.myapptest.ui.room.RoomMainFragment"
        android:label="fragment_room_main"
        tools:layout="@layout/fragment_room_main" >
        <action
            android:id="@+id/action_roomMainFragment_to_roomNoteFragment"
            app:destination="@id/roomNoteFragment" />
    </fragment>
    <fragment
        android:id="@+id/roomNoteFragment"
        android:name="com.game.myapptest.ui.room.RoomNoteFragment"
        android:label="fragment_room_note"
        tools:layout="@layout/fragment_room_note" />
    <fragment
        android:id="@+id/roomRegisterFragment"
        android:name="com.game.myapptest.ui.room.RoomRegisterFragment"
        android:label="fragment_room_register"
        tools:layout="@layout/fragment_room_register" >
        <action
            android:id="@+id/action_roomRegisterFragment_to_roomMainFragment"
            app:destination="@id/roomMainFragment" />
        <action
            android:id="@+id/action_roomRegisterFragment_to_roomLoginFragment"
            app:destination="@id/roomLoginFragment" />
    </fragment>
</navigation>